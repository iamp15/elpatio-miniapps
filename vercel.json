{
  "rewrites": [
    {
      "source": "/depositos",
      "destination": "/depositos/index.html"
    },
    {
      "source": "/retiros",
      "destination": "/retiros/index.html"
    },
    {
      "source": "/salas",
      "destination": "/salas/index.html"
    },
    {
      "source": "/configuracion",
      "destination": "/configuracion/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOWALL"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors *"
        }
      ]
    }
  ]
}
